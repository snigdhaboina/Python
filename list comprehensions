
def findpairs(x, y, z, N):
 list = []
 
 for i in range(0,x+1):
        for j in range(0,y+1):
            for k in range(0, z+1):
                if(i+j+k != N) :
                    list.append([i,j,k])
                    
 return list


x = int(input("enter x"))
y = int(input("enter y"))
z = int(input("enter z"))
N = int(input("enter N"))

print(findpairs(x,y,z,N))
